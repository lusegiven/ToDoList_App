{
    "version": 2,
    "builds": [
        { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "/login" },
        { "src": "/(.*)", "dest": "/create-account" },
        { "src": "/(.*)", "dest": "/get-user" },
        { "src": "/(.*)", "dest": "/add-todo" }
    ]
  }
  